{
  "entryPoints": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "out": "docs",
  "exclude": [
    "**/*.test.js",
    "**/*.test.jsx",
    "**/node_modules/**",
    "**/coverage/**",
    "**/build/**"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "theme": "default",
  "name": "Mortgage Calculator Documentation",
  "readme": "README.md",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Components",
    "Hooks", 
    "Utils",
    "Redux",
    "Pages",
    "Services",
    "*"
  ],
  "defaultCategory": "Other",
  "plugin": [],
  "jsDocCompatibility": true,
  "validation": {
    "invalidLink": true,
    "notExported": true
  },
  "tsconfig": "tsconfig.json"
} 